}
QListView::item
{
    padding-top:12px;
    padding-bottom:6px;
}

QListView::item:selected
{    /*border:0px;*/
    background:#F4F6FF;
	color:rgba(0,0,0,0.65);  
    padding:0px;
    margin:0px;
}/*上次选择后保留的状态，鼠标离开后显示*/QListView::item:selected:!active
{
    border-width:0px;
    background:#F4F6FF;
}
QScrollBar:vertical
{
    width:3px;
    background:#ffffff;
    margin:0px,0px,0px,0px;
    padding-top:0px;   /*上预留位置*/
    padding-bottom:0px;    /*下预留位置*/}/*滚动条中滑块的样式*/
QScrollBar::handle:vertical
{
    width:3px;
    background:rgba(0,0,0,0.15);
    border-radius:4px;
    min-height:20px;}/*鼠标触及滑块样式*/QScrollBar::handle:vertical:hover
{
    width:4px;
	height:0px;
    background:rgb(0,0,0,50%);
    border-radius:4px;
    min-height:0;}
/*设置下箭头*/QScrollBar::add-line:vertical
{
    height:0px;
    width:0px;
    subcontrol-position:bottom;}
/*设置上箭头*/QScrollBar::sub-line:vertical
{
    height:0px;
    width:0px;
    subcontrol-position:top;}
/*设置下箭头:悬浮状态*/
QScrollBar::add-line:vertical:hover
{
    height:12px;
    width:0px;
    subcontrol-position:bottom;}
/*设置上箭头：悬浮状态*/
QScrollBar::sub-line:vertical:hover
{
    height:0px;
    width:0px;
    subcontrol-position:top;
}

</string>
